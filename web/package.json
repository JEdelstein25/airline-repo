{
	"name": "@stellora/airline-web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": "22.12.0",
		"pnpm": "9.15.1"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "npm run -s test:e2e && npm run -s test:unit -- --run",
		"test:e2e": "playwright test",
		"test:unit": "vitest",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write .",
		"gen": "npm run -s gen:openapi",
		"gen:openapi": "openapi-typescript ../api/airline.openapi.yaml --output src/lib/airline.openapi.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.6",
		"@types/node": "^22.10.2",
		"@vitest/ui": "^2.1.8",
		"autoprefixer": "^10.4.20",
		"bits-ui": "1.0.0-next.74",
		"clsx": "^2.1.1",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"jsdom": "^25.0.1",
		"lucide-svelte": "^0.469.0",
		"openapi-typescript": "^7.4.4",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.9",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^6.0.5",
		"vitest": "^2.1.8"
	},
	"overrides": {
		"@polka/url": "1.0.0-next.28"
	},
	"dependencies": {
		"openapi-fetch": "^0.13.3"
	}
}
